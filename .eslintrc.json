{
    "extends": "airbnb-base",
    "plugins": [
        "import",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "linebreak-style": "off",
        "no-unused-vars": [2, { "varsIgnorePattern": "h" }]
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true
    }
}